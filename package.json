{
  "name": "papaja-template",
  "publisher": "alpkaanaksu",
  "displayName": "papaja template",
  "description": "Creates papaja (R Markdown) folder structure for research projects",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "papaja-template.newProjectNewFolder",
      "title": "papaja: New project (create new folder)"
    },
    {
      "command": "papaja-template.newProjectCurrentFolder",
      "title": "papaja: New project (create template in current directory)"
    }
  ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.79.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
